  object uptIndicadores: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM indicadores'
      'WHERE'
      '  indicadores.ID = :OLD_ID')
    InsertSQL.Strings = (
      'INSERT INTO indicadores'
      
        '  (ID, DataImportacao, Arquivo, IDPapel, CodPapel, Cotacao, Prec' +
        'oLucro, '
      
        '   PrecoVlrPratinomial, PrecoReceitaLiquida, Div_Yield, PrecoAti' +
        'vo, PrecoCapGiro, '
      
        '   PrecoEBIT, PrecoAtivCircLiq, EVEbit, EVEBITIDA, MrgEbit, Marg' +
        'Liq, LiqCorrente, '
      
        '   ROIC, ROE, Liq2Meses, PatrimLiq, DivBrutaPatrimLiq, CrescRec5' +
        'a, LPA, '
      
        '   VPA, Margem_Bruta, EBIT_Ativo, Liquidez_Corr, Giro_Ativos, Da' +
        'taUltCot, '
      '   VolMed2Meses)'
      'VALUES'
      
        '  (:ID, :DataImportacao, :Arquivo, :IDPapel, :CodPapel, :Cotacao' +
        ', :PrecoLucro, '
      
        '   :PrecoVlrPratinomial, :PrecoReceitaLiquida, :Div_Yield, :Prec' +
        'oAtivo, '
      
        '   :PrecoCapGiro, :PrecoEBIT, :PrecoAtivCircLiq, :EVEbit, :EVEBI' +
        'TIDA, :MrgEbit, '
      
        '   :MargLiq, :LiqCorrente, :ROIC, :ROE, :Liq2Meses, :PatrimLiq, ' +
        ':DivBrutaPatrimLiq, '
      
        '   :CrescRec5a, :LPA, :VPA, :Margem_Bruta, :EBIT_Ativo, :Liquide' +
        'z_Corr, '
      '   :Giro_Ativos, :DataUltCot, :VolMed2Meses)')
    ModifySQL.Strings = (
      'UPDATE indicadores SET'
      '  ID = :ID,'
      '  DataImportacao = :DataImportacao,'
      '  Arquivo = :Arquivo,'
      '  IDPapel = :IDPapel,'
